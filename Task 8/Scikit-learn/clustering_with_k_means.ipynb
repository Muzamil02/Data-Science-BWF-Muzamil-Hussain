        "from sklearn.cluster import KMeans\n",
        "import numpy as np\n",
        "\n",
        "X = np.array([[1, 2], [2, 3], [3, 4], [5, 6], [8, 8]])\n",
        "\n",
        "kmeans = KMeans(n_clusters=2)\n",
        "kmeans.fit(X)\n",
        "\n",
        "print(\"Cluster Centers:\\n\", kmeans.cluster_centers_)"
